            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="81.0" text="Status" fx:id="PREQS_statusIDCol" />
          </columns>
        </TableView>
      </labelFor>
    </Label>
    <fx:reference source="pReqsTable" />
    <Button fx:id="checkPurchaseButton" layoutX="411.0" layoutY="275.0" mnemonicParsing="false" onMouseClicked="#checkPurchaseButtonOnClicked" text="Check purchase" />
  </children>
</AnchorPane>
