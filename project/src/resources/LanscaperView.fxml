            <TableColumn maxWidth="5000.0" minWidth="10.0" prefWidth="98.0" text="Request ID" fx:id="CREQS_reqIDCol" />
            <TableColumn prefWidth="75.0" text="plantID" fx:id="CREQS_plantIDCol" />
            <TableColumn prefWidth="75.0" text="Type" fx:id="CREQS_typeCol" />
            <TableColumn prefWidth="75.0" text="Status" fx:id="CREQS_statusCol" />
          </columns>
        </TableView>
      </labelFor>
    </Label>
    <fx:reference source="cReqsTable" />
